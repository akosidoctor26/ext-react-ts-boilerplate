{
  "manifest_version": 3,
  "name": "<APP_NAME>",
  "description": "<APP_DESCRIPTION>",
  "version": "1.0.0",
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ]
}
